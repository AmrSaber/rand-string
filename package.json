{
  "name": "@amrsaber/rand-string",
  "version": "0.2.1",
  "description": "CLI tool that generates and outputs random string of choice",
  "main": "cli.js",
  "author": "Amr Saber",
  "repository": "https://github.com/AmrSaber/rand-string",
  "license": "MIT",
  "scripts": {
    "publish": "npm publish --access public",
    "bump:major": "npm version major --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:patch": "npm version patch --no-git-tag-version",
    "postversion": "git add -- package.json package-lock.json && git commit -m \":arrow_up: Bump version to $npm_package_version\" && git tag \"v$npm_package_version\""
  },
  "keywords": [
    "cli",
    "random",
    "string",
    "random-string",
    "random string",
    "random-generator",
    "random generator",
    "generator"
  ],
  "dependencies": {
    "esm": "^3.2.25",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "bin": {
    "rand-string": "./src/cli.js"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  }
}

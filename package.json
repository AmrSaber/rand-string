{
  "name": "random-generator-cli",
  "version": "2.4.1",
  "description": "CLI tool that generates and outputs random string of choice",
  "main": "src/cli.js",
  "author": {
    "name": "Amr Saber",
    "email": "Amr.M.Saber.mail@gmail.com"
  },
  "repository": "https://github.com/AmrSaber/random-cli",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/",
    "bump:major": "npm version major --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:patch": "npm version patch --no-git-tag-version",
    "postversion": "git add -- package.json package-lock.json && git commit -m \":arrow_up: Bump version to $npm_package_version\""
  },
  "keywords": [
    "cli",
    "random",
    "string",
    "number",
    "integer",
    "float",
    "boolean",
    "array",
    "secure",
    "secure-random",
    "secure-generator",
    "secure-random-generator",
    "random-string",
    "random-number",
    "random-integer",
    "random-float",
    "random-boolean",
    "random-array",
    "random-generator",
    "generator"
  ],
  "dependencies": {
    "esm": "^3.2.25",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/yargs": "^16.0.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-esm-transformer": "^1.0.0",
    "jest-extended": "^0.11.5",
    "node-run-cmd": "^1.0.1"
  },
  "bin": {
    "random": "./src/cli.js"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "transform": {
      ".js": "jest-esm-transformer"
    }
  }
}
